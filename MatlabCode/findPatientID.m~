function ptID = findPatientID (rcrd)
    switch (rcrd)
        case 's0010_rem'
            ptID = 'ID01_';
        case 's0014lrem'
            ptID = 'ID01';
        case 's0016lrem'
            ptID = 'ID01';
        case 's0015lrem'
            ptID = 'ID02';
        case 's0017lrem'
            ptID = 'ID03';
        case 's0020arem'
            ptID = 'ID04';
        case 's0020brem'
            ptID = 'ID04';
        case 's0021arem'
            ptID = 'ID05';
        case 's0021brem'
            ptID = 'ID05';
        case 's0025lrem'
            ptID = 'ID05';
        case 's0031lrem'
            ptID = 'ID05';
        case 's0101lrem'
            ptID = 'ID05';
        case 's0022lrem'
            ptID = 'ID06';
        case 's0027lrem'
            ptID = 'ID06';
        case 's0026lrem'
            ptID = 'ID07';
        case 's0029lrem'
            ptID = 'ID07';
        otherwise
            ptID = 'NA';
    end
end